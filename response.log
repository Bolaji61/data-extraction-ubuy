    function autocompleteSearch(searchterm = '',notavailable = null) {

        if(searchterm == '' || searchterm == null){
            searchterm = $('#new-search-suggestions-box .search-field').val().trim();
        }

        if(searchterm.length < 3 || searchterm.length > 50) {
            show_suggestion = false;
            showDefault();
            $('#search-form-sm .search-box-text').val(searchterm);
            $('#search-form .search-box-text-hm').val(searchterm);
            return;
        }
        if (searchterm == '' || searchterm.length < 3) {
            hideAutocomplete();
            return;
        }

        if (searchterm == lastSearchTerm) {
            showAutocomplete(recent_response);
            return;
        }

        lastSearchTerm = searchterm;

        if(stored_data.length>0) {
            var visible_suggestion =  getDataFromStoredKeyword(searchterm);
            if(visible_suggestion) {
                return;
            }
        }
        var count_len=0;
        xhr = jQuery.ajax({
            type: 'POST',
            url: 'https://www.you-buy.ca/en/ubcommon/auto-complete/suggestions',
            data: {s_id: 81, q: searchterm, csrftoken: csrf_token},
            dataType: 'json',
            beforeSend: function () {
                if (xhr != null) {
                    xhr.abort();
                }
                if (xhr2 != null) {
                    xhr2.abort();
                }
            },
            success: function (response) {
                if (typeof response != 'undefined' && response.success) {
                    count_len  = response.suggestions.length
                    keyword_suggestion = recent_response = makeAutoComplete(response.suggestions);
                    showAutocomplete(keyword_suggestion);
                    if(response.count==0)  {
                        show_suggestion = false;
                        show_popular_search = false;
                        jQuery('#recent-search-listing').html(getRecentKeyword());
                        jQuery('#recent-search-listing').removeClass('d-none');
                        jQuery('#popular-listing').removeClass('d-none');
                        $('.load-all-products').addClass('d-none');
                        makePopularSearchKeyword();
                    }

                    if(notavailable != null && notavailable == 'yes') {
                            var suggestions_li = '';
                            jQuery("#notavailable_related_search ul").remove('li');
                            suggestions_li += '<li class="heading">Related Searches  - </li>';
                            jQuery.each(response.suggestions, function (index, value) {
                                if(index < 7) {
                                   suggestions_li += '<li><a href="https://www.you-buy.ca/en/search/?q=' + encodeURIComponent(jQuery.trim(value)) + '&ref=sug-sr" >' + value + '</a></li>';
                               }
                                
                            });
                            jQuery("#notavailable_related_search").html(suggestions_li);
                        }
                    
                    
                } else {
                    hideAutocomplete();
                }
                if(count_len != 0 && xhr2.status == 200) {
                    saveDataInStoredKeyWord();
                }
            },
            error: function (data, statusCode) {
                hideAutocomplete();
            }
        }).done(function ( data, textStatus, jqXHR) {
            if(jqXHR.status == 204) {
                location.reload();
            } 
        });
        autocompleteSearch_related_product(search_term);
        $('#search-form-sm .search-box-text').val(search_term);
        $('#search-form .search-box-text-hm').val(search_term);

    }

    function getRecentKeyword() {
        if(show_suggestion) {
            return false;
        }
        var new_session_id = JSON.parse(localStorage.getItem('new_session_id'));        
        var cookieId = getCacheWithExpiry('new_session_id');
        
        var uba_lastThree = '';
        var getRecentArr = localStorage.getItem('recent_' + uba_cookieId);
        
        var uba_lastThree = JSON.parse(getRecentArr);


        if (uba_lastThree != '' && uba_lastThree != undefined && uba_lastThree.length >= 2) {
            uba_lastThree = uba_lastThree.reverse();
