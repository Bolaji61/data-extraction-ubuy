                }
                if(count_val != 0 && xhr.status == 200) {
                    saveDataInStoredKeyWord();
                }
                
            },
            error: function () {
                hideAutocomplete();
            }
        }).done(function ( data, textStatus, jqXHR) {
            if(jqXHR.status == 204) {
                location.reload();
            } 
        });
        $('#search-form-sm .search-box-text').val(search_term);
        $('#search-form .search-box-text-hm').val(search_term);
    }

    function scrollAutocompleteSearch(e) {
        var search_suggestions = jQuery('#new-search-suggestions-box .new-search-suggestions li');
        if (!search_suggestions) {
            return;
        }

        var key = e.keyCode,
                selected = search_suggestions.filter('.selected'),
                current;
        if (key != 40 && key != 38) {
            return;
        }

        search_suggestions.removeClass('selected');
        if (key == 40) { // Down key
            if (!selected.length || selected.is(':last-child')) {
                current = search_suggestions.eq(0);
            } else {
                current = selected.next();
            }
        } else if (key == 38) { // Up key
            if (!selected.length || selected.is(':first-child')) {
                current = search_suggestions.last();
            } else {
                current = selected.prev();
            }
        }
        current.addClass("selected");
        var selected_suggestions = jQuery('#new-search-suggestions-box .new-search-suggestions').find('li.selected').text();
        if (typeof selected_suggestions !== typeof undefined && selected_suggestions != '') {
            jQuery('#new-search-suggestions-box .search-field').val(selected_suggestions);
            jQuery('#search-form-sm .search-box-text').val(selected_suggestions);
            jQuery('#search-form .search-box-text-hm').val(selected_suggestions);
        }
    }
