        n = new RegExp('^'+search_keyword);
        var data_fetched = false;
        new_data = stored_data.filter( (a)=> a.keyword.toString().match(n) );
        new_data.sort(sortByKeywordLength);
        if(new_data.length>0) {
            
            if( new_data[0].keyword.length - search_keyword.length <=3) {
                recent_response = new_data[0].suggestion;
