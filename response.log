<script>

    var ubNewsletterCouponCodeBlock = jQuery("#ub-newsletter-coupon-code");
    function destroyNewsletterCouponCode() {
        resetNewsletterCouponCodePopup("#ub-newsletter-coupon-code");
        ubNewsletterCouponCodeBlock.modal('hide');
    }

    function resetNewsletterCouponCodePopup(popupElmId) {
        var ub_coupon_block = jQuery(popupElmId).find('.ub-coupon-block');
        ub_coupon_block.css({backgroundColor: '#f6b11b'});
        var couponcode = jQuery('ccode').text();
        ub_coupon_block.find('span').remove();
        ub_coupon_block.find('.fa-copy').css({cursor: 'pointer'});
        ub_coupon_block.find('.fa-copy').bind('click', function () {
            copyNewsletterCouponCode(couponcode)
        });
    }

    function showNewsletterCouponCode() {
        ubNewsletterCouponCodeBlock.modal('show');
    }
    function copyNewsletterCouponCode(text) {
        if (newcopyToClipboard(text)) {
            var element = event.target;
            jQuery(element).unbind('click');
            element.removeAttribute("onclick");
//            element.onclick = function () {
//                return false;
//            };
            element.style.cursor = "auto";
            var eleParent = element.parentNode;
            eleParent.style.backgroundColor = "#7BBB05";
            eleParent.insertAdjacentHTML('beforeend', '<span style="float:right;font-size: 15px;color: #fff;margin: 5px;">Copied</span>');
        }
        return true;
    }

    function fallbackCopyToClipboard(text) {
        if (window.clipboardData && window.clipboardData.setData) {
            // Internet Explorer-specific code path to prevent textarea being shown while dialog is visible.
            return clipboardData.setData("Text", text);

        } else if (document.queryCommandSupported && document.queryCommandSupported("copy")) {
            var textArea = document.createElement("textarea");
            // textarea.textContent = text;
            textArea.value = text;
            //textarea.style.position = "fixed";  // Prevent scrolling to bottom of page in Microsoft Edge.
            document.body.appendChild(textArea);
            textArea.focus();
            textArea.select();
            jQuery('ccode').select();
            try {
                return document.execCommand("copy");  // Security exception may be thrown by some browsers.
            } catch (ex) {
                console.warn("Copy to clipboard failed.", ex);
                return false;
            } finally {
                document.body.removeChild(textArea);
            }
        }
    }

    function newcopyToClipboard(text) {
        if (!navigator.clipboard) {
            return fallbackCopyToClipboard(text);
        }
        navigator.clipboard.writeText(text).then(function () {
            return true;
        }, function (err) {
            return false;
        });
        return true;

    }

</script>
